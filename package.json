{
    "name": "my-todo",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://github.com/TishaKot/todo-list.git",
    "dependencies": {
        "@emotion/react": "^11.13.5",
        "@emotion/styled": "^11.13.5",
        "@mui/material": "^6.1.9",
        "prop-types": "^15.8.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-scripts": "5.0.1",
        "styled-components": "^5.3.11",
        "uuid": "^11.0.5"
    },
    "scripts": {
        "format": "prettier --write .",
        "start": "react-scripts start",
        "build": "react-scripts build",
